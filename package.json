{
  "name": "new-test-module",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "commitlint --edit --extends seek",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "compile": "babel src --out-dir dist --ignore '*.test.js'",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "format-check": "prettier --list-different 'src/**/*.js'",
    "test": "jest"
  },
  "author": "Discover",
  "license": "UNLICENSED",
  "homepage": "https://github.com/SEEK-Jobs/new-test-module#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "seek"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "commitizen": "2.9.6",
    "commitlint-config-seek": "^1.0.0",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^0.14.3",
    "semantic-release": "^12.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserslist-config-seek": "^1.0.0",
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-seek": "^3.0.1",
    "prettier": "^1.10.2",
    "jest": "^22.2.1",
    "renovate-config-seek": "^0.2.0"
  }
}
